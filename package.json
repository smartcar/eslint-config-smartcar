{
  "name": "@smartcar/eslint-config",
  "version": "0.0.0-semantic-release",
  "description": "Shareable eslint config for smartcar",
  "keywords": [
    "eslint",
    "smartcar",
    "lint",
    "style"
  ],
  "homepage": "https://github.com/smartcar/eslint-config-smartcar#readme",
  "bugs": {
    "url": "https://github.com/smartcar/eslint-config-smartcar/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smartcar/eslint-config-smartcar.git"
  },
  "license": "MIT",
  "author": {
    "name": "Smartcar",
    "email": "hello@smartcar.com",
    "url": "https://smartcar.com"
  },
  "main": "index.js",
  "files": [
    "ava.js",
    "index.js",
    "lodash.js",
    "mocha.js"
  ],
  "scripts": {
    "check-rules": "eslint-config-prettier index.js",
    "lint": "eslint . --cache && sort-package-json --check && prettier --check .",
    "lint:fix": "eslint . --cache --fix && sort-package-json && prettier --write .",
    "prepare": "husky install",
    "test": "ava",
    "posttest": "npm run -s check-rules"
  },
  "dependencies": {
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-array-func": "3.1.7",
    "eslint-plugin-ava": "13.2.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.3.3",
    "eslint-plugin-lodash": "7.4.0",
    "eslint-plugin-mocha": "10.0.5",
    "eslint-plugin-n": "15.2.4",
    "eslint-plugin-no-use-extend-native": "0.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-sonarjs": "0.13.0",
    "eslint-plugin-unicorn": "43.0.1"
  },
  "devDependencies": {
    "@smartcar/prettier-config": "2.0.0",
    "ava": "4.3.1",
    "eslint": "8.19.0",
    "husky": "^8.0.0",
    "is-plain-obj": "3.0.0",
    "lodash.difference": "4.5.0",
    "prettier": "2.7.1",
    "semantic-release": "^19.0.3",
    "sort-package-json": "1.57.0"
  },
  "peerDependencies": {
    "eslint": ">=8.19.0"
  },
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=8.3.0"
  }
}
